{
    "version": 2,
    "builds": [
      {
        "src": "api/screenshot.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/screenshot.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}